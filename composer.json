{
    "name": "ctrlwebinc/voyager-modals",
    "description": "Arrrrr! A package to implement modals into Voyager Pages and Voyager Page Blocks",
    "license": "MIT",
    "authors": [
        {
            "name": "ctrlweb",
            "email": "info@ctrlweb.ca",
            "homepage": "https://ctrlweb.ca/"
        }
    ],
    "homepage": "https://github.com/ctrlwebinc/voyager-modals",
    "keywords": ["Laravel", "Voyager", "Voyager Pages", "Voyager Page Blocks", "Modals"],
    "require": {
        "illuminate/support": "~5",
        "tcg/voyager": "^1.1",
        "pvtl/voyager-frontend": "^0.8.0",
        "pvtl/voyager-pages": "^0.2.0",
        "pvtl/voyager-page-blocks": "^0.8.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "illuminated/testing-tools": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Ctrlweb\\VoyagerModals\\": "src/",
            "Ctrlweb\\VoyagerModals\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ctrlweb\\VoyagerModals\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ctrlweb\\VoyagerModals\\VoyagerModalsServiceProvider"
            ],
            "aliases": {
                "VoyagerModals": "Ctrlweb\\VoyagerModals\\Facades\\VoyagerModals"
            }
        }
    }
}
